
<form action="{{ path('produit_produit_panier_addnew_line', {'id': panier.id}) }}" method="post">
    <div class="form-group">
        <select name="produitOrganisationId" id="produit_organisation" class="form-control input-lg">
            {% for produitOrganisation in liste_produitOrganisation %}
            <option value="{{ produitOrganisation.id }}">{{ produitOrganisation.produit.nom }}</option>
            {% endfor %}
        </select>
    </div>

    <div class="form-group">
        <button type="submit" class="btn btn-primary">
            Enregistrer et continuer <span class="bi bi-arrow-return-right"></span>
        </button>
    </div>
</form>


<link href="{{ asset('templatedash/css/select2.min.css') }}" rel="stylesheet"/>
<script src="{{ asset('templatedash/js/select2.min.js') }}"></script>

<script type="text/javascript">
var $disabledResults = $("#produit_organisation");
$disabledResults.select2();
</script>