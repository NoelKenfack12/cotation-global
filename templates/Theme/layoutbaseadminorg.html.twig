  <style>
    .tabs-step {
      list-style: none;
      padding: 0;
      border: 1px solid #1e4a8b;
    }
    @media all and (min-width: 48em) {
      .tabs-step {
        margin: 0 0;
        display: table;
        width: 100%;
        height: 40px;
      }
    }
    .tabs-step li {
      text-align: left;
      width: 100%;
      border-top: 1px solid #1e4a8b;
      margin: 0;
      position: relative;
      padding: 1em 0;
      box-sizing: border-box;
      color: #1e4a8b;
    }
    .tabs-step li:first-child {
      border-top: 0;
    }
    @media all and (min-width: 48em) {
      .tabs-step li {
        display: table-cell;
        width: auto;
        vertical-align: middle;
        padding: 0 0 0 15px;
        /* left padding should compensate for arrow width */
        margin: 0;
        border: none;
      }
      .tabs-step li:before {
        content: "";
        position: absolute;
        left: calc(100% + 2px);
        top: 3px;
        width: 0;
        height: 0;
        border-top: 21px solid transparent;
        border-left: 15px solid #1e4a8b;
        border-bottom: 21px solid transparent;
        z-index: 10;
      }
      .tabs-step li:after {
        content: "";
        position: absolute;
        left: 99%;
        /* IE8 and Android don't do calc so well */
        left: calc(100% + 1px);
        top: 4px;
        width: 0;
        height: 0;
        border-top: 20px solid transparent;
        border-left: 14px solid white;
        border-bottom: 20px solid transparent;
        z-index: 11;
      }
      .tabs-step li:last-child:before, .tabs-step li:last-child:after {
        border: none;
      }
    }
    .tabs-step li a {
      color: #1e4a8b;
      text-decoration: none;
      border: none;
      display: block;
      padding-top: 1px;
      padding-left: 15px;
    }
    .tabs-step li:hover {
      background-color: #ffe6e6;
    }
    .tabs-step li:hover:after {
      border-left: 14px solid #ffe6e6;
    }
    .tabs-step li.active {
      background-color: #1e4a8b;
      color: white;
    }
    .tabs-step li.active:after {
      border-left: 14px solid #8f9090;
    }
    .tabs-step li.active a {
      color: white;
    }

    .card-dash-csu tr td input{
